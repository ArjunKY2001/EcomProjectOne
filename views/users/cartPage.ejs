            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center clean">
                                <% if(cart){ %>
                                <thead>
                                    <p <% if (!cart.items.length <= 0) { %> style="display: none;" <% } %>>The cart is empty</p>

                                    <tr class="main-heading">
                                        <% if(cart.items.length !=0){ %>
                                        <th scope="col">Image</th>
                                        <th scope="col">Name</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Quantity</th>
                                        <th scope="col">Subtotal</th>
                                        <th scope="col">Remove</th>
                                        <% } %>
                                    </tr>
                                </thead>
                                <% }else{ %>
                                    <p>The cart is empty</p>
                                    <% } %>
                                <tbody>
                                    <% if(cart){ %>

                                    <% for(let i=0;i<cart.items.length;i++){ %>
                                        <tr>
                                            <td class="image product-thumbnail"> <% if (cart.items[i].product && cart.items[i].product.images && cart.items[i].product.images.length > 0) { %>
                                                <img src="/<%=cart.items[i].product.images[0] %>" alt="#">
                                            <% } else { %>
                                                <span>No Image Available</span>
                                            <% } %></td>
                                            <td class="product-des product-name">
                                                <% if (cart.items[i].product && cart.items[i].product.name) { %>
                                                    <h5 class="product-name"><a href="shop-product-right.html"><%=cart.items[i].product.name %></a></h5>
                                                <% } else { %>
                                                    <span>No Product Name Available</span>
                                                <% } %>
                                            </td>
                                            
                                            <td class="price" data-title="Price">
                                                
                                                <span>$<span class="product-price"><%=cart.items[i].product.price %></span></span>
                                            </td>
                                            <td class="text-center" data-title="Stock">
                                                <div class="row">
                                                    <div class="col">
                                                        <div class="input-group mb-3 d-flex align-items-center quantity-container" style="max-width: 100px;">
                                                            <div class="btn-group" role="group" aria-label="Quantity">
                                                                <button class="btn1 decrease" type="button" id="buttonMinus" <%if(cart.items[i].quantity<=1){%>style="display: none;" <% } %> onclick="cartQuantityminus('<%= i %>')">&minus;</button>
                                                                <div class="form-control text-center quantity-amount" id="diplayquanty<%=i %>"><%= cart.items[i].quantity %></div>
                                                                <button class="btn1 increase" type="button" id="buttonPlus" onclick="cartQuantityplus('<%= i %>')">&plus;</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                
                                            </td>
                                            <td class="text-right" data-title="Cart">
                                                <span class="total-price" id="subtotal<%=i %>">$<%= cart.items[i].subtotal %></span>
                                            </td>
                                            
                                            <td class="action" data-title="Remove">
                                                <a  onclick="confirmDelete(`<%= cart.items[i].id %>`)"  class="text-muted remove-item" data-item-id="">
                                                    <i class="fi-rs-trash"></i>
                                                </a>
                                            </td>
                                            
                                        </tr>
                                    <% } %>
                                    <% } %>
                                    
                                </tbody>
                            </table>
                            <% if(cart){ %>
                           <% let totalPrice = 0; %>
                            <%for(let i=0;i<cart.items.length;i++){ %>
                                <% if (cart.items[i].product && cart.items[i].product.images && cart.items[i].product.images.length > 0) { %>
                                    <% } else { %>
                                    <% } %></td>
                                    <% if (cart.items[i].product && cart.items[i].product.name) { %>
                                        <%=cart.items[i].product.name %></a></h5>
                                    <% } else { %>
                                    <% } %>
                                <td><%= cart.items[i].quantity * cart.items[i].product.price %></td>
                                <% totalPrice += cart.items[i].quantity * cart.items[i].product.price %>
                            <% } %>
                            <% } %>
                        </div>
                        <div class="cart-action text-end">
