    <script>
        
    function cartQuantityminus(index) {
        const subtotalDisplay = document.getElementById(`subtotal${index}`);
        const quantityDisplayminus =document.getElementById(`diplayquanty${index}`)
    fetch(`/user/quantityDecrease?index=${index}`, {
        method: 'GET'
    })
    .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok');
                    }
                    return response.json();
    })
    .then(data => {
        console.log(data);
        quantityDisplayminus.textContent=data.items[index].quantity
        subtotalDisplay.textContent=data.items[index].subtotal
        if(data.items[index].quantity===1){
    setTimeout(()=>{
        window.location.reload(); // Refresh only when the fetch is successful
    },100)
}
    })
   
}

function cartQuantityplus(index) {
    const subtotalDisplay = document.getElementById(`subtotal${index}`);
    const quantityDisplayplus = document.getElementById(`diplayquanty${index}`)
    fetch(`/user/quantityIncrease?index=${index}`, {
        method: 'GET'
    })
    .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok');
                    }
                    return response.json();
    })
    .then(data=>{
        console.log(data.index);
        quantityDisplayplus.textContent = data.cart.items[data.index].quantity
         // Assuming you have a subtotal display element
        subtotalDisplay.textContent = "$"+ data.cart.items[data.index].subtotal;
        if(data.cart.items[data.index].quantity===2){
    setTimeout(()=>{
        window.location.reload(); // Refresh only when the fetch is successful
    },100)
}
    })
}
    </script>
